<%= form_for @collection, url: user_collections_path, html: {multipart: true} do |f| %>

	<%= f.label :title %>
	<%= f.text_field :title %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :image %>
	<%= f.file_field :image %>

	<%= f.submit 'Add Collection' %>

<% end %>